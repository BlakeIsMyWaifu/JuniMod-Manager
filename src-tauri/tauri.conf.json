{
  "productName": "junimod-manager",
  "version": "0.0.0",
  "identifier": "dev.blakeismywaifu.junimod-manager",
  "build": {
    "beforeDevCommand": "vite",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "tsc && vite build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "JuniMod Manager",
        "minWidth": 1280,
        "width": 1920,
        "minHeight": 720,
        "height": 1080,
        "fullscreen": false,
        "maximized": true,
        "resizable": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}